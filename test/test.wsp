type TestType [
  a: i32
  b: i32
] {
  add(): i32 {
    -> a + b
  }
}

func testConditions(number: i32): u1 {
    if [number == 0] {
        -> 1
    } else if [number == 1] {
        -> 1
    } else {
        -> 0
    }
}

func testLoops(): u1 {
    num: i32 = 2
    for [i: i32 = 1, i < 15, i++] {
        num = TestType[i num].add()
    }
    -> testConditions(num)
}
