type TestType [
  data a: i32
  data b: i32
] {
  func add(): i32 {
    -> a + b
  }
}

func testConditions(number: i32): u1 {
    if [number == 0] {
        -> 1
    } else if [number == 1] {
        -> 1
    } else {
        -> 0
    }
}

data max: i32 = 15

func testLoops(): u1 {
    data num: i32 = 2
    for [data i: i32 = 1, i < max, i++] {
        num = TestType[i num].add()
    }
    -> testConditions(num)
}
